{
  "AppSettings": {
    "Secret": "Sup3rS3cr3tSup3rV3ryL0ng"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:dd/MM/yyyy HH:mm:ss.fff} [{Level}] [{Application}/{ApplicationVersion}] {Message}{NewLine}{Exception}"
        }
      }
    ],
    "AllowedHosts": "*"
  }
}
